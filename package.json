{
  "name": "smart-sheet-connect",
  "version": "1.0.0",
  "description": "Frontend architecture and design system for SmartSheetConnect - a production-ready lead capture platform built with React, TypeScript, and Tailwind CSS.",
  "type": "module",
  "files": [
    "client",
    "shared",
    "config",
    "dist",
    "*.config.*",
    "*.json",
    "*.md",
    "LICENSE"
  ],
  "license": "MIT",
  "author": "Lindsey Stead",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "tsc --noEmit",
    "check:watch": "tsc --noEmit --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "clean": "rm -rf dist node_modules/.vite .vite coverage .nyc_output",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "reinstall": "npm run clean:all && npm install",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "postinstall": "echo 'Installation complete. Run npm run dev to start development server.'"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
